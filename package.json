    "@eslint/js": "^9.11.1",
    "@svgr/webpack": "^8.1.0",
    "@types/leaflet": "^1.9.16",
    "@types/lunr": "^2.3.7",
    "@types/node": "22.14.0",
    "@types/qs": "^6",
    "@types/react": "^18.3.9",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.11.1",
    "eslint-config-next": "^14.2.13",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
