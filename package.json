{
  "name": "template",
  "private": true,
  "packageManager": "yarn@4.6.0",
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next start",
    "build": "next build",
    "lint": "eslint src && tsc --noEmit",
    "lint:fix": "eslint src --fix && tsc --noEmit",
    "lint:strict": "eslint --max-warnings=0 && tsc --noEmit",
    "format": "prettier --write . --log-level silent"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint src --fix",
      "prettier --write --log-level silent"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier --write --log-level silent"
    ]
  },
  "dependencies": {
