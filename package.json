    "axios": "^1.7.7",
    "better-auth": "^1.2.8",
    "better-sqlite3": "^11.10.0",
    "dompurify": "^3.2.4",
    "lucide-react": "^0.511.0",
    "next": "^15.3.1",
    "postcss": "^8.5.3",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-loading-skeleton": "^3.5.0",
    "react-responsive": "^10.0.0",
    "react-spinners": "^0.17.0",
